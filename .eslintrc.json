{
    "plugins": ["jest", "@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:@typescript-eslint/recommended"],
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module",
      "project": "./tsconfig.json"
    },
    "rules": {
      "semi": ["warn", "always"],
      "@typescript-eslint/no-unused-vars": "error"
    },
    "env": {
      "node": true,
      "es6": true,
      "jest/globals": true
    }
  }
